<section class="cta">
  <div class="section-container">
  	<% if already_submitted? %>
	    <%= link_to :submit, :class => 'btn btn-large btn-secondary' do %><span>submit another pic</span><% end %>
	    <%= link_to :mine, :class => 'btn btn-large btn-primary' do %><span>see your pets</span><% end %>
    <% else %>
      <%= link_to :intent, :class => 'btn btn-large' do %><span>submit your pic</span><% end %>
    <% end %>
  </div>
</section>
