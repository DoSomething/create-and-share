<section class="utility-bar">
  <% if Rails.env.development? %><%= link_to 'debug', '#', { :id => 'debug' } %><% end %>
  <% if authenticated? %>
    <%= link_to 'log out', :logout, { :class => 'logout' } %>
  <% else %>
    <%= link_to 'log in', :login, { :class => 'logout' } %>
  <% end %>
  <%= link_to 'faq', :faq, { :class => 'faq' } %>
  <% if admin? %>
    <%= link_to 'edit this campaign', edit_campaign_path(get_campaign.id) %>
  <% end %>
</section>
