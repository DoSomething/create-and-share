<section class="utility-bar">
  <% if Rails.env.development? %><%= link_to 'debug', '#', { :id => 'debug' } %><% end %>
  <%= link_to_if authenticated?, 'log out', :logout, { :class => 'logout' } %>
  <%= link_to_unless authenticated?, 'log in', :login, { :class => 'logout' } %>
  <%= link_to 'faq', :faq, { :class => 'faq' } %>
</section>
